import stdlib.*;

struct Thing {
  v! Vec3i;
}

struct Vec3i {
  x int;
  y int;
  z int;
}

func bork(thing &!Thing, v Vec3i) void {
  set thing.v = v;
}

exported func main() {
  thing = Thing(Vec3i(7, 8, 9));
  println(thing.v.y); // => 8
  bork(&!thing, Vec3i(4, 5, 6));
  println(thing.v.y); // => 5
}
