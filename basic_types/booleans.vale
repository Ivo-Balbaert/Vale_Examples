import stdlib.*;

exported func main() {
  bt = true;
  bf = false;

  row = 2;
  println(row == 2);       // true
  b = (row == 2);
  println(b);              // true

  println(bt);             // => true
  println(not bt);         // => false

  println(bt and bf);      // => false
  println(bt or bf);       // => true

  println(bt and not bf);  // => true
}